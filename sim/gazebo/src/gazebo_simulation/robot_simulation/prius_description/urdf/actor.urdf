  <joint name="actor_steering_joint" type="continuous">
    <origin xyz="0.357734 -0.627868 0.988243" rpy="-1.302101 0 0"/>
    <parent link="actor_chassis"/>
    <child link="actor_steering_wheel"/>
    <axis xyz="0 0 1"/>
    <limit lower="-7.85" upper="7.85" effort="10000000" velocity="1000000"/>
  </joint>

  <joint name="actor_front_left_steer_joint" type="continuous">
    <parent link="actor_chassis"/>
    <child link="actor_fl_axle"/>
    <origin xyz="0.767 -1.41 0.3" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>
  <joint name="actor_front_right_steer_joint" type="continuous">
    <parent link="actor_chassis"/>
    <child link="actor_fr_axle"/>
    <origin xyz="-0.767 -1.41 0.3" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>
  <joint name="actor_front_left_wheel_joint" type="continuous">
    <parent link="actor_fl_axle"/>
    <child link="actor_front_left_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="actor_front_right_wheel_joint" type="continuous">
    <parent link="actor_fr_axle"/>
    <child link="actor_front_right_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
